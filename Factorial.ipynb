   "source": [
    "# Given a positive integer, return its factorial.\n",
    "\n",
    "def integer(i):\n",
    "    fact=1\n",
    "    for i in range(1,i+1):\n",
    "        fact=fact*i\n",
    "    return fact\n",
    "    "
